{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/club/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/club/tsconfig.app.json"
  },
  "projects": {
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "authentification": {
      "type": "application",
      "root": "apps/authentification",
      "entryFile": "main",
      "sourceRoot": "apps/authentification/src",
      "compilerOptions": {
        "tsConfigPath": "apps/authentification/tsconfig.app.json"
      }
    },
    "reservations": {
      "type": "application",
      "root": "apps/reservations",
      "entryFile": "main",
      "sourceRoot": "apps/reservations/src",
      "compilerOptions": {
        "tsConfigPath": "apps/reservations/tsconfig.app.json"
      }
    },
    "marketplace": {
      "type": "application",
      "root": "apps/marketplace",
      "entryFile": "main",
      "sourceRoot": "apps/marketplace/src",
      "compilerOptions": {
        "tsConfigPath": "apps/marketplace/tsconfig.app.json"
      }
    },
    "blog": {
      "type": "application",
      "root": "apps/blog",
      "entryFile": "main",
      "sourceRoot": "apps/blog/src",
      "compilerOptions": {
        "tsConfigPath": "apps/blog/tsconfig.app.json"
      }
    },
    "managementclub": {
      "type": "application",
      "root": "apps/managementclub",
      "entryFile": "main",
      "sourceRoot": "apps/managementclub/src",
      "compilerOptions": {
        "tsConfigPath": "apps/managementclub/tsconfig.app.json"
      }
    },
    "apigateway": {
      "type": "application",
      "root": "apps/apigateway",
      "entryFile": "main",
      "sourceRoot": "apps/apigateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/apigateway/tsconfig.app.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/club"
}